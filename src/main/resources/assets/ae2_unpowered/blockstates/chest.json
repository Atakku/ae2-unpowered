{
  "multipart": [
    {
      "apply": {
        "model": "ae2_unpowered:block/chest/base"
      }
    },
    {
      "when": {
        "lights_on": false
      },
      "apply": {
        "model": "ae2_unpowered:block/chest/lights_off"
      }
    },
    {
      "when": {
        "lights_on": true
      },
      "apply": {
        "model": "ae2_unpowered:block/chest/lights_on"
      }
    }
  ]
}
